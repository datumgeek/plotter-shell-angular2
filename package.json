{
  "name": "plotter-shell-angular2",
  "version": "0.2.4",
  "license": "MIT",
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "copy": "copyfiles -u 1 ./src/**/*.less ./dist",
    "tsc": "./node_modules/.bin/tsc",
    "clean": "./node_modules/.bin/rimraf \"./dist\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "clean-seed": "rimraf ../plotter-app-seed-angular2/node_modules/plotter-shell-angular2/dist",
    "deploy-seed": "copyfiles ./dist/**/* ../plotter-app-seed-angular2/node_modules/plotter-shell-angular2",
    "build": "npm run clean && npm run copy && npm run tsc && npm run clean-seed && npm run deploy-seed"
  },
  "private": false,
  "dependencies": {
    "@angular/common": "2.1.0",
    "@angular/compiler": "2.1.0",
    "@angular/core": "2.1.0",
    "@angular/forms": "2.1.0",
    "@angular/http": "2.1.0",
    "@angular/platform-browser": "2.1.0",
    "@angular/platform-browser-dynamic": "2.1.0",
    "@angular/router": "3.1.0",
    "plotter-shell-model": "^0.1.1",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-rc.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.25",
    "bootstrap": "^3.3.7",
    "es6-shim": "^0.35.1",
    "font-awesome": "^4.6.3",
    "fontawesome": "^4.5.0",
    "html2canvas": "^0.5.0-beta4",
    "jquery": "^2.2.3",
    "reflect-metadata": "^0.1.8",
    "toastr": "^2.1.2"
  },
  "devDependencies": {
    "canonical-path": "0.0.2",
    "codelyzer": "~0.0.26",
    "concurrently": "^2.0.0",
    "copyfiles": "^1.0.0",
    "es6-promise": "^4.0.5",
    "http-server": "^0.9.0",
    "lite-server": "^2.2.0",
    "lodash": "^4.11.1",
    "tslint": "3.15.1",
    "typescript": "2.0.3",
    "typings": "^0.8.1"
  }
}
